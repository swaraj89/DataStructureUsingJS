{
    "name": "dsjs",
    "version": "1.0.0",
    "description": "DS using JS",
    "main": "./dist/index.js",
    "scripts": {
        "init": "mkdir dist",
        "build": "babel ./src -d ./dist --ignore test.js",
        "test": "mocha --compilers js:babel-core/register ./test/**/*.test.js",
        "test:single": "mocha --compilers js:babel-core/register",
        "pretest": "npm run build",
        "start": "npm run dev",
        "dev": "npm test -- -w"
    },
    "author": "Swaraj Panigrahi",
    "devDependencies": {
        "babel-cli": "^6.16.0",
        "babel-core": "^6.17.0",
        "babel-preset-es2015": "^6.24.1",
        "chai": "^3.5.0",
        "mocha": "^3.1.2"
    }
}